	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	
	<!-- display & choosable is deactivated for now (always TRUE), omitted -->
	<modes>
		<mode type="tree">
			<parent nodetype="sbCMS:Layout">
				<child nodetype="sbJukebox:TPL_TopRated" />
				<child nodetype="sbCMS:Template" />
				<child nodetype="sbCMS:TPL_Menu" />
				<child nodetype="sbJukebox:TPL_MostHeard" />
				<child nodetype="sbJukebox:TPL_Favorites" />
				<child nodetype="sbJukebox:TPL_NowPlaying" />
				<child nodetype="sbGuestbook:TPL_Entries" />
			</parent>
			<parent nodetype="sbCMS:Page">
				<child nodetype="sbCMS:Page" />
				<child nodetype="sbGuestbook:Guestbook" />
			</parent>
			<parent nodetype="sbCMS:Site">
				<child nodetype="sbForum:Forum" />
				<child nodetype="sbGuestbook:Guestbook" />
				<child nodetype="sbJukebox:Jukebox" />
				<child nodetype="sbCMS:Page" />
			</parent>
		</mode>
		<mode type="create">
			<parent nodetype="sbCMS:Layout">
				<child nodetype="sbCMS:TPL_Menu" />
				<child nodetype="sbJukebox:TPL_TopRated" />
				<child nodetype="sbJukebox:TPL_NowPlaying" />
				<child nodetype="sbGuestbook:TPL_Entries" />
				<child nodetype="sbJukebox:TPL_Favorites" />
				<child nodetype="sbJukebox:TPL_MostHeard" />
			</parent>
			<parent nodetype="sbCMS:Page">
				<child nodetype="sbGuestbook:Guestbook" />
				<child nodetype="sbCMS:Page" />
				<child nodetype="sbForum:Forum" />
			</parent>
			<parent nodetype="sbCMS:Site">
				<child nodetype="sbJukebox:Jukebox" />
				<child nodetype="sbForum:Forum" />
				<child nodetype="sbGuestbook:Guestbook" />
				<child nodetype="sbCMS:Page" />
			</parent>
		</mode>
		<mode type="gatherContent">
			<parent nodetype="sbCMS:CTN_Container">
				<child nodetype="sbCMS:CTN_Image" />
				<child nodetype="sbCMS:CTN_Plaintext" />
				<child nodetype="sbCMS:CTN_Richtext" />
			</parent>
			<parent nodetype="sbCMS:Layout">
				<child nodetype="sbCMS:TPL_Menu" />
			</parent>
			<parent nodetype="sbCMS:Page">
				<child nodetype="sbCMS:CTN_Container" />
				<child nodetype="sbCMS:CTN_Plaintext" />
				<child nodetype="sbCMS:CTN_Image" />
				<child nodetype="sbCMS:CTN_Richtext" />
			</parent>
		</mode>
		<mode type="choosePage">
			<parent nodetype="sbCMS:Page">
				<child nodetype="sbForum:Forum" />
				<child nodetype="sbGuestbook:Guestbook" />
				<child nodetype="sbCMS:Page" />
			</parent>
			<parent nodetype="sbCMS:Site">
				<child nodetype="sbForum:Forum" />
				<child nodetype="sbGuestbook:Guestbook" />
				<child nodetype="sbCMS:Page" />
			</parent>
		</mode>
		<mode type="list">
			<parent nodetype="sbCMS:Page">
				<child nodetype="sbCMS:Page" />
			</parent>
			<parent nodetype="sbCMS:Site">
				<child nodetype="sbCMS:Page" />
				<child nodetype="sbForum:Forum" />
			</parent>
		</mode>
	</modes>
	
